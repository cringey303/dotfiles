    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": [
                    "comment",
                    "punctuation.definition.comment"
                ],
                "settings": {
                    "foreground": "#808a93",
                    "fontStyle": "italic"
                }
            },
            {
                "scope": [
                    "string",
                    "punctuation.definition.string"
                ],
                "settings": {
                    "foreground": "#a6c9a2"
                }
            },
            {
                "scope": [
                    "keyword",
                    "storage.type",
                    "storage.modifier"
                ],
                "settings": {
                    "foreground": "#82aaff"
                }
            },
            {
                "scope": [
                    "constant.numeric",
                    "constant.language"
                ],
                "settings": {
                    "foreground": "#f78c6cc4"
                }
            },
            {
                "scope": [
                    "entity.name.function",
                    "support.function"
                ],
                "settings": {
                    "foreground": "#c2a9f2"
                }
            },
            {
                "scope": [
                    "variable",
                    "entity.name.variable",
                    "parameter.variable"
                ],
                "settings": {
                    "foreground": "#dde4ea"
                }
            }
        ]
    },
    "custom-ui-style.css": [
        "div.activitybar { background-color: #4a545c !important; }",
        ".activitybar { background-color: #4a545c !important; }",
        ".monaco-workbench .activitybar { background-color: #4a545c !important; }",
        "#workbench\\.parts\\.activitybar { background-color: #4a545c !important; }"
    ],
    "files.autoSave": "afterDelay",
    "workbench.startupEditor": "none",
    "code-runner.runInTerminal": true,
    "liveServer.settings.donotShowInfoMsg": true,
    "latex-workshop.latex.tools": [
        {
            "name": "latexmk",
            "command": "latexmk",
            "args": [
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "-pdf",
                "-outdir=%OUTDIR%",
                "%DOC%"
            ],
